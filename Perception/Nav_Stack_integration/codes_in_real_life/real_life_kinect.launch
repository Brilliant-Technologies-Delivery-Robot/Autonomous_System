<launch>
    <!-- Launch camera_stream_sim node -->
    <node name="kinect_reallife_stream" pkg="kinect_in_reallife" type="kinect_frame_pub.py" output="screen">
    </node>

    <!-- Launch camera_sub_sim node -->
    <node name="kinect_detect" pkg="kinect_in_reallife" type="kinect_frame_sub_detect.py" output="screen">
    </node>

    <!-- Launch sub_detgepthimage node -->
    <node name="kinect_get_depth" pkg="kinect_in_reallife" type="sub_detgepthimage.py" output="screen">
    </node>
    
    <node name="pixel_meter_conversion" pkg="kinect_in_reallife" type="convert_error_to_meter.py" output="screen">
    </node>
</launch>
