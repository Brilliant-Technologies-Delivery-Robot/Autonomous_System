<?xml version='1.0'?>

<robot name="delivery_robot_urdf" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:property name="robot_name" value="delivery_robot_urdf" />
	<xacro:property name="robot_chassis_mass" value="4.1401"/>
	<xacro:property name="robot_chassis_length" value="0.5"/>
	<xacro:property name="robot_chassis_radius" value="0.13"/>
	<xacro:property name="robot_caster_wheel_radius" value="0.05"/>
	<xacro:property name="robot_caster_wheel_radius_collision" value="0.0499"/>

	<xacro:property name="robot_wheel_mass" value="1"/>
	<xacro:property name="robot_wheel_length" value="0.05"/>
	<xacro:property name="robot_wheel_radius" value="0.13"/>

<!--	<xacro:property name="camera_mass" value="0.1"/>-->
	<xacro:property name="hokoyu_mass" value="1e-5"/>

	<!-- Make Chassis of Bot -->
	<link name="chassis">
		<pose>0 0 0.1 0 0 0</pose>

		<inertial>
			<mass value="${robot_chassis_mass}"/>
      		<origin xyz="-0.00049444 1.0033E-14 0.11158" rpy="0 0 0" />

      		<inertia 
      			ixx="0.07173"
			ixy="-1.0013E-10"
			ixz="3.1516E-06"
			iyy="0.070159"
			iyz="1.5491E-17"
			izz="0.13789"/>
		</inertial>

		<collision name="collision">
			<origin xyz="0 0 0" rpy=" 0 0 0"/>
		      <geometry>
			<mesh
			  filename="package://delivery_robot_urdf/meshes/base_link.STL" />
		      </geometry>
		</collision>

		<visual name="chassis_visual">
			<origin xyz="0 0 0" rpy=" 0 0 0"/>
		      <geometry>
			<mesh
			  filename="package://delivery_robot_urdf/meshes/base_link.STL" />
		      </geometry>

		</visual>

	</link>


	<!-- Right Wheel -->
	<link name="right_wheel">
		<inertial>
			<mass value="0.15849"/>
			      <origin xyz="-2.2582E-16 -6.9389E-17 0.02815" rpy="0 0 0" />
			<inertia
				ixx="0.00022462"
				ixy="1.3198E-19"
				ixz="-2.3706E-20"
				iyy="0.00022462"
				iyz="-4.7562E-20"
				izz="0.00043866"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		        <geometry>
			  <mesh
			  filename="package://delivery_robot_urdf/meshes/right_wheel.STL" />
		        </geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
			  <mesh
			  filename="package://delivery_robot_urdf/meshes/right_wheel.STL" />
			</geometry>
		</collision>

	</link>

	<!-- left_wheel-->
	<link name="left_wheel">
		<inertial>
			<mass value="0.15849"/>
			      <origin xyz="-2.2906E-16 9.7145E-17 0.02815" rpy="0 0 0" />
			<inertia
				ixx="0.00022462"
				ixy="1.8208E-19"
				ixz="2.0093E-20"
				iyy="0.00022462"
				iyz="5.4464E-20"
				izz="0.00043866"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		        <geometry>
			  <mesh
			  filename="package://delivery_robot_urdf/meshes/left_wheel.STL" />
		        </geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
			  <mesh
			  filename="package://delivery_robot_urdf/meshes/left_wheel.STL" />
			</geometry>
		</collision>

	</link>

	<!-- Camera -->
<!--	<link name="camera">
		<inertial>
			<mass value="${camera_mass}"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia
		        ixx="1e-6" ixy="0.0" ixz="0.0"
		        iyy="1e-6" iyz="0.0"
		        izz="1e-6"
		      />
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.05"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.05"/>
			</geometry>
		</collision>
	</link>
-->
	<!-- Hokuyo Lidar -->
	<link name="hokuyo">
		<inertial>
			<mass value="${hokoyu_mass}"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>

			<inertia
		        ixx="1e-6" ixy="0.0" ixz="0.0"
		        iyy="1e-6" iyz="0.0"
		        izz="1e-6"
		      />
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://delivery_robot_sim/meshes/hokuyo.dae"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.1 0.1"/>
			</geometry>
		</collision>
	</link>

	<!-- Project center to the ground -->
	<link name="robot_footprint"></link>



	<!-- Define Joints -->

	<!-- right_wheel_joint-->
	<joint type="continuous" name="right_wheel_joint">
	    <origin xyz="0 -0.197748 0.0670500000000001" rpy="1.5707963267949 0 0"/>
	    <parent link="chassis"/>
	    <child link="right_wheel" />
	    <axis xyz="0 0 -1" />
	    <limit effort="10000" velocity="1000" />
	    <dynamics damping="1.0" friction="1.0" />
	</joint>

	<!-- left_wheel_joint-->
	<joint type="continuous" name="left_wheel_joint">
	    <origin xyz="0 0.197748 0.0670500000000001" rpy="-1.5707963267949 0 0"/>
	    <parent link="chassis"/>
	    <child link="left_wheel" />
	    <axis xyz="0 0 1"/>
	    <limit effort="10000" velocity="1000" />
	    <dynamics damping="1.0" friction="1.0" />
	</joint>

	<!-- Camera Joint -->
<!--	<joint name="camera_joint" type="fixed">
		<origin xyz="0.26 0 0" rpy="0 0 0" />
		<parent link="chassis"/>
		<child link="camera" />
		<axis xyz="0 1 0"/>
	</joint>
-->
	<!-- Hokoyu Joint -->
	<joint name="hokuyo_joint" type="fixed">
	    <origin xyz="0 0 0.26" rpy="0 0 0" />
	    <parent link="chassis"/>
	    <child link="hokuyo" />  
	    <axis xyz="0 1 0"/> 
	</joint>

	<joint name="robot_footprint_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0" />
		<parent link="robot_footprint"/>
		<child link="chassis" />
	</joint>


	<!-- Color of bot -->
	<gazebo reference="left_wheel">
		<material>Gazebo/Green</material>
		<kp>1000000.0</kp> <!-- kp and kd for rubber -->
		<kd>100.0</kd>
		<mu1>1.0</mu1>
		<mu2>1.0</mu2>
		<maxVel>1.0</maxVel>
		<minDepth>0.00</minDepth>
	</gazebo>

	<gazebo reference="right_wheel">
		<material>Gazebo/Green</material>
		<kp>1000000.0</kp> <!-- kp and kd for rubber -->
		<kd>100.0</kd>
		<mu1>1.0</mu1>
		<mu2>1.0</mu2>
		<maxVel>1.0</maxVel>
		<minDepth>0.00</minDepth>
	</gazebo>
<!--	<gazebo reference="camera">
		<material>Gazebo/Red</material>
	</gazebo>
-->
	<gazebo reference="chassis">
		<material>Gazebo/Blue</material>
	</gazebo>

	<!-- Motor, Camera and Lidar Simulation -->
	<xacro:include filename="$(find delivery_robot_sim)/urdf/delivery_robot.gazebo" />

</robot>
